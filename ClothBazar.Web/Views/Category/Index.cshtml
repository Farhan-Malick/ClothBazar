@model List<ClothBazar.Entities.Category>
@{
    ViewBag.Title = "Index";
}

<h2>Categories</h2>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var category in Model)
            {
                <tr>
                    <td>@category.name</td>
                    <td>@category.Description</td>
                </tr>
            }
        </tbody>
    </table>
